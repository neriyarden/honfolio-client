{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "nonExistantCommand": {
      "dependsOn": ["^nonExistantCommand"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "test": {
      "inputs": ["**/*.tsx", "**/*.ts", "**/*.test.ts", "**/*.test.tsx"]
    },
    "lint": {
      "dependsOn": ["^nonExistantCommand"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^nonExistantCommand"]
    }
  }
}
